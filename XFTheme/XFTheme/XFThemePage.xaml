<?xml version="1.0" encoding="utf-8"?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
		xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
		xmlns:local="clr-namespace:XFTheme"
		x:Class="XFTheme.XFThemePage">
	<StackLayout>
		<Label Text="FontColor1 Text..." TextColor="{StaticResource FontColor1}" VerticalOptions="FillAndExpand" HorizontalOptions="Center" />
		<Label Text="Color1 Text..." TextColor="{StaticResource Color1}" VerticalOptions="FillAndExpand" HorizontalOptions="Center" />
		<Label Text="Color2 Text..." TextColor="{StaticResource Color2}" VerticalOptions="FillAndExpand" HorizontalOptions="Center" />
		<Label Text="Color3 Text..." TextColor="{StaticResource Color3}" VerticalOptions="FillAndExpand" HorizontalOptions="Center" />
		<Label Text="Color4 Text..." TextColor="{StaticResource Color4}" VerticalOptions="FillAndExpand" HorizontalOptions="Center" />
	</StackLayout>
</ContentPage>
